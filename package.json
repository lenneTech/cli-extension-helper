{
  "name": "@lenne.tech/cli-plugin-helper",
  "version": "0.0.8",
  "description": "Helper plugin for Gluegun CLI Projects",
  "keywords": [
    "lenne.Tech",
    "cli",
    "gluegun",
    "plugin",
    "extension"
  ],
  "contributors": [
    "Pascal Klesse",
    "Kai Haase <kai.haase@lenne.tech> (http://lenne.tech)"
  ],
  "homepage": "http://lenne.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/cli-plugin-helper"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/cli-plugin-helper/issues"
  },
  "scripts": {
    "build": "npm run lint && npm run clean-build && npm run compile",
    "clean-build": "rimraf ./dist",
    "compile": "tsc -p .",
    "coverage": "jest --coverage",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "lint": "eslint --ext .js,.ts src",
    "prepack": "npm run build",
    "preversion": "npm run lint",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@lenne.tech/gluegun-menu": "0.0.2"
  },
  "devDependencies": {
    "@lenne.tech/npm-package-helper": "0.0.10",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "eslint": "6.5.0",
    "eslint-config-prettier": "6.3.0",
    "find-file-up": "2.0.1",
    "gluegun": "4.0.0",
    "husky": "3.0.7",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  },
  "peerDependencies": {
    "find-file-up": ">=2",
    "gluegun": ">=3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node --skip-project extras/sync-version.ts && pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "LICENSE",
    "assets/demo.gif",
    "README.md"
  ]
}
